<prescription xmlns="http://greenify.github.io/schemas/prescription/v3" type="activity" sender="other">
  <intent-filter>
    <action name="android.intent.action.VIEW" />
    <scheme name="alipays" />
    <ssp sglob="//platformapi/startapp?.*qrcode.*" />
  </intent-filter>
    <intent-filter>
    <action name="android.intent.action.VIEW" />
    <scheme name="intent" />
    <ssp sglob="//platformapi/startapp?.*qrcode.*alipay.*" />
  </intent-filter>
  <intent-filter>
    <action name="android.intent.action.VIEW" />
    <scheme name="http" />
    <ssp sglob="//qr.alipay.com/.*" />
  </intent-filter>
    <intent-filter>
    <action name="android.intent.action.VIEW" />
    <scheme name="market" />
    <ssp prefix="//details?id=com.eg.android.AlipayGphone" />
  </intent-filter>
  <intent-filter>
    <action name="android.intent.action.VIEW" />
    <scheme name="http" />
    <ssp sglob="//market.android.com/.*com.eg.android.AlipayGphone.*" />
  </intent-filter>
</prescription>